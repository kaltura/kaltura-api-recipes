<h1>Search Results for support</h1>
<hr></hr>
<% result.forEach(function(caption, index) { %>
  <div class="row">
    <div class="col-xs-4 text-right">
      <img src="<%- caption.entry.thumbnailUrl %>"></img>
    </div>
    <div class="col-xs-7 col-xs-offset-1">
      <p>
        <i><%- caption.content %></i>
      </p>
    </div>
  </div>
  <hr></hr>
<% }); %>