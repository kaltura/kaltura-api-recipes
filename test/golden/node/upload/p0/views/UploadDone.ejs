<script>
  $('#UploadStatus').text('Uploaded file <%- result.fileName %>. Creating entry...');
</script>

<div class="KalturaMediaEntry"></div>
<script>
  var element = $('.KalturaMediaEntry').last();
  element[0].loadData = function() {
    $('.KalturaMediaEntry').last().load('CreateMediaEntry', {
       name: <%- JSON.stringify(result.fileName) %>,
       uploadTokenId: <%- JSON.stringify(result.id) %>,
    });
  }
  element[0].loadData();
</script>